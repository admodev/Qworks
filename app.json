{ 
    "expo": {
        "name": "quedeoficios",
        "description": "App para profesionales que ofrecen sus servicios y personas que desean contratarlos.",
        "slug": "quedeoficios",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#654F6F"
        },
        "notification": { 
            "icon": "./assets/icon.png"
        }, 
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["assets/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.admodev.quedeoficios",
            "icon": "./assets/icon.png"
        },
        "android": {
            "versionCode": 1,
            "useNextNotificationsApi": true,
            "package": "com.admodev.quedeoficios",
            "icon": "./assets/icon.png"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
