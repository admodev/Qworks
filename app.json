{
  "expo": {
    "name": "Q works!",
    "description": "Q works! es localizar en tiempo real, un proveedor de servicios.",
    "slug": "qworks",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#654F6F"
    },
    "notification": {
      "icon": "./assets/icon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.admodev.qworks",
      "buildNumber": "1.0.0",
      "icon": "./assets/icon.png",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to let user put a photo in his profile page."
      }
    },
    "android": {
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE", "CAMERA_ROLL"],
      "versionCode": 10,
      "useNextNotificationsApi": true,
      "package": "com.admodev.qworks",
      "icon": "./assets/icon.png"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
