{ 
  "expo": {
    "name": "quedeoficiosfinal",
    "slug": "quedeoficiosfinal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#654F6F",
      "firebase": {
        "apiKey": "${ FIREBASE_AUTH_DOMAIN }",
        "authDomain": "${ FIREBASE_AUTH_DOMAIN }",
        "databaseURL": "${ FIREBASE_DATABASE_URL }",
        "projectId": "${ FIREBASE_PROJECT_ID }",
        "storageBucket": "${ FIREBASE_STORAGE_BUCKET }",
        "messagingSenderId": "${ FIREBASE_MESSAGING_SENDER_ID }",
        "appId": "${ FIREBASE_APP_ID }",
        "measurementId": "${ FIREBASE_MEASUREMENT_ID }"
      },
      "googleMaps": {
        "apiKey": "${ GOOGLE_MAPS_API_KEY}"
      },
      "facebookScheme": "${ FACEBOOK_SCHEME }",
      "facebookAppId": "${ FACEBOOK_APP_ID }",
      "facebookDisplayName": "${ FACEBOOK_DISPLAY_NAME }"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.admodev.quedeoficios"
    },
    "android": {
      "versionCode": 1,
      "useNextNotificationsApi": true,
      "package": "com.admodev.qdo2020"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "config": {
        "firebase": {
          "apiKey": "${ FIREBASE_API_KEY }",
          "authDomain": "${ FIREBASE_AUTH_DOMAIN }",
          "databaseURL": "${ FIREBASE_DATABASE_URL }",
          "projectId": "${ FIREBASE_PROJECT_ID }",
          "storageBucket": "${ FIREBASE_STORAGE_BUCKET }",
          "messagingSenderId": "${ FIREBASE_MESSAGING_SENDER_ID }",
          "appId": "${ FIREBASE_APP_ID }",
          "measurementId": "${ FIREBASE_MEASUREMENT_ID }"
        },
        "googleMaps": {
          "apiKey": "${ GOOGLE_MAPS_API_KEY}"
        },
        "facebookScheme": "${ FACEBOOK_SCHEME }",
        "facebookAppId": "${ FACEBOOK_APP_ID }",
        "facebookDisplayName": "${ FACEBOOK_DISPLAY_NAME }"
      }
    }
  }
}
